{
	"name": "nest-typescript-starter",
	"version": "1.0.0",
	"description": "Nest TypeScript starter repository",
	"license": "MIT",
	"scripts": {
		"start": "node index.js",
		"dev": "nodemon",
		"prestart:prod": "tsc",
		"start:prod": "node dist/main.js",
		"test": "NODE_ENV=test jest --config=jest.json",
		"test:watch": "NODE_ENV=test jest --watch --config=jest.json",
		"test:coverage": "NODE_ENV=test jest --config=jest.json --coverage --coverageDirectory=coverage",
		"e2e": "NODE_ENV=test jest --config=e2e/jest-e2e.json --forceExit",
		"e2e:watch": "NODE_ENV=test jest --watch --config=e2e/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^4.6.6",
		"@nestjs/core": "^4.6.6",
		"@nestjs/microservices": "^4.6.6",
		"@nestjs/swagger": "^1.1.4",
		"@nestjs/testing": "^4.6.6",
		"@nestjs/typeorm": "^2.0.0",
		"@nestjs/websockets": "^4.6.6",
		"bcryptjs": "^2.4.3",
		"class-validator": "^0.8.1",
		"cors": "^2.8.4",
		"jsonwebtoken": "^8.2.0",
		"lodash": "^4.17.5",
		"mysql": "^2.15.0",
		"nodemailer": "^4.6.2",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"redis": "^2.8.0",
		"reflect-metadata": "^0.1.12",
		"rxjs": "^5.5.7",
		"typeorm": "^0.1.16",
		"typescript": "^2.7.2"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.1",
		"@types/jest": "^22.2.0",
		"@types/lodash": "^4.14.104",
		"@types/node": "^9.4.7",
		"jest": "^23.0.0-alpha.0",
		"nodemon": "^1.17.2",
		"supertest": "^3.0.0",
		"ts-jest": "^22.4.1",
		"ts-node": "^5.0.1"
	}
}
